{
  "output_dir": "./downloads",
  "format": "opus",
  "bitrate": "192k",
  "concurrency": 1,
  "ffmpeg_path": "ffmpeg",
  "spotdl_extra_args": [],
  "postprocess": {
    "convert_with_ffmpeg": true,
    "normalize_audio": false
  },
  "retry_delay": 10,
  "filename_template": "{artist} - {title}.{ext}",
  "notify": {
    "enabled": false,
    "method": "ntfy",
    "ntfy_topic": ""
  },
  "retry": {
    "attempts": 3,
    "backoff_seconds": 10
  },
  "schedule_interval_hours": 12
}